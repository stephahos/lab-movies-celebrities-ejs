<h2>Celebrities</h2>
<a href="/celebrities/new">Add celebrity</a>

<% celebrities.forEach(celebrity => { %>
<div style="border: 1px solid black; margin-bottom: 1rem; padding: 0 20px 20px">
        <h4><%= celebrity.name %></h4>
        <h4><%= celebrity.occupation %></h4>
        <h4><%= celebrity.catchPhrase %></h4>
</div>
<% }) %>